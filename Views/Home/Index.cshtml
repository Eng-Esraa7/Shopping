                </div>
            </div>
            <!-- Indicators -->
            <ol class="carousel-indicators">
                <li data-target="#my" data-slide-to="0" class="active">
                    <img src="~/images/testimonial/pic1.jpg" alt="Ahmed">
                </li>
                <li data-target="#my" data-slide-to="1">
                    <img src="~/images/testimonial/pic2.jpg" alt="Sara">
                </li>
                <li data-target="#my" data-slide-to="2">
                    <img src="~/images/testimonial/pic3.jpg" alt="Eslam">
                </li>
                <li data-target="#my" data-slide-to="3">
                    <img src="~/images/testimonial/pic4.jpg" alt="Rana">
                </li>
            </ol>

        </div>
        <!--End carousel-->

    </div>
</div>
<!--End testimonial-->
@*start comment
<section class="comment text-center">
    <div class="container">
        <p style="color:#ff0000">Write Comment</p>
        <form class="form-group">
            <input type="text" placeholder="Your comment">
            <button class="btn btn-danger" type="submit">Send<i class="fas fa-edit fa-lg"></i></button>
        </form>
    </div>
</section>
End comment*@
