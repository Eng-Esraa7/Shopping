@{
    ViewBag.Title = "Home Page";
}


<!--star carousel-->
<div id="myslide" class="carousel slide" data-ride="carousel">
    <!-- Indicators -->
    <ol class="carousel-indicators">
        <li data-target="#myslide" data-slide-to="0" class="active"></li>
        <li data-target="#myslide" data-slide-to="1"></li>
        <li data-target="#myslide" data-slide-to="2"></li>
        <li data-target="#myslide" data-slide-to="3"></li>
        <li data-target="#myslide" data-slide-to="4"></li>
        <li data-target="#myslide" data-slide-to="5"></li>
    </ol>

    <!-- Wrapper for slides -->
    <div class="carousel-inner" role="listbox" id="home">
        <div class="item active">
            <img src="~/images/shopping/sh6.jpg" alt="pic1">
            <div class="carousel-caption hidden-xs">
                <h2 class="h1">Welcome In Our society</h2>
                <p class="lead">Iam Happy To Visit My Website</p>
            </div>
        </div>

        <div class="item">
            <img src="~/images/shopping/sh2.jpg" alt="pic2">
            <div class="carousel-caption hidden-xs hidden-xs">
                <h2 class="h1">You will get your Purpose</h2>
            </div>
        </div>

        <div class="item">
            <img src="~/images/shopping/sh3.jpeg" alt="pic3">
            <div class="carousel-caption hidden-xs">
                <h2 class="h1">Thanks for our clients</h2>
            </div>
        </div>

        <div class="item">
            <img src="~/images/shopping/sh4.jpg" alt="pic4">
            <div class="carousel-caption hidden-xs">
                <h2>not difficult to achieve your aim but difficult to keep on it</h2>
            </div>
        </div>

        <div class="item">
            <img src="~/images/shopping/sh5.jpg" alt="pic5">
            <div class="carousel-caption hidden-xs">
                <h2>Interested!</h2>
            </div>
        </div>

        <div class="item">
            <img src="~/images/shopping/sh1.jpg" alt="pic6">
            <div class="carousel-caption hidden-xs">
                <h2>Thanks for our clients</h2>
            </div>

        </div>

        <!-- Controls -->
        <a class="left carousel-control" href="#myslide" role="button" data-slide="prev">
            <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
            <!--glyphicon glyphicon-chevron-left icon in bookstrap-->
            <span class="sr-only">Previous</span>
        </a>
        <a class="right carousel-control" href="#myslide" role="button" data-slide="next">
            <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>
</div>
<!--End carousel-->
<!--start intro-->
<div class="intro text-center wow animate__shakeX" data-wow-duration="20s" data-wow-offset="100" data-wow-itration="2" id="intro" style="background-color: rgb(73 75 75 / 49%);">
    <div class="container">
        <h1>Welcome In Our Website <span>Shopping</span></h1>
        <p class="lead">We Have All Types Of clothes And Accessories.<br>You Will Get Your Purpose.</p>
    </div>
</div>
<!--End intro-->
<!--start feature-->
<div class="feat text-center" id="feat">
    <div class="container">
        <h2 class="h1">Our Features</h2>
        <div class="row">
            <div class="col-md-4 col-xs-12 wow animate__backInDown" data-wow-duration="3s" data-wow-offset="80">
                <i class="fas fa-users fa-3x"></i>
                <h2>Website Easy to use</h2>
                <p class="lead">Our site is easy to any one to use, speed to loading.</p>
            </div>

            <div class="col-md-4 col-xs-12 wow animate__backInUp" data-wow-duration="3s" data-wow-offset="80">
                <i class="fas fa-tshirt fa-3x"></i>
                <h2>Less Price High Quality</h2>
                <p class="lead">Our site is characterized by nice products, High Quality, Less Price</p>
            </div>

            <div class="col-md-4 col-xs-12 wow animate__backInDown" data-wow-duration="3s" data-wow-offset="80">
                <i class="fas fa-users fa-3x"></i>
                <h2>The latest fashion</h2>
                <p class="lead">We always Download The latest fashion all over the world</p>
            </div>
        </div>
    </div>
</div>
<!--End feature-->
<!--start stats-->
<section class="stats text-center" id="stat">
    <div class="color">
        <div class="container">
            <h2 class="h1">Our Statistic</h2>
            <div class="row">
                <div class="col-md-4">
                    <div class="data hvr-pulse-grow">
                        <i class="fas fa-users fa-5x"></i>
                        <p>125,135</p>
                        <span>Visits Sites </span>
                    </div>
                </div>

                <div class="col-md-4">
                    <div class="data hvr-pulse-grow">
                        <i class="fas fa-suitcase fa-5x"></i>
                        <p>20,800</p>
                        <span>Sales</span>
                    </div>
                </div>

                <div class="col-md-4">
                    <div class="data hvr-pulse-grow">
                        <i class="fas fa-comments fa-5x"></i>
                        <p>50,600</p>
                        <span>Comments </span>
                    </div>
                </div>

            </div>
        </div>
    </div>
</section>
<!--End stats-->
<!--start testimonial-->
